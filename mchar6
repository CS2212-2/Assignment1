// program to find the GPA By Mishon Charles
#include <stdio.h>
#include <conio.h>

int main()
{
  int n = 6, i;
  float mark[6]={3.1,3.6,2.0,3.5,3.2,2.9};
  float sum = 0.0;
  float gpa = 0;

  for(i = 0; i < n; i++)
  {
      sum = sum + mark[i];

  }
  gpa  = sum / n;
   printf ("G.P.A = %.2f", gpa);
   return 0;

}
/*
G.P.A = 3.05
Process returned 0 (0x0)   execution time : 0.016 s
Press any key to continue.
*/
